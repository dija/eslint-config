{
  "name": "@dijateam/eslint-config",
  "version": "2.1.0",
  "description": "Taj ESLint and Prettier Configs for Javascript and Typescript packages.",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "lint",
    "config",
    "prettier"
  ],
  "author": "Dija Team <info@dija.sa>",
  "license": "MIT",
  "repository": "https://github.com/dijateam/eslint-config",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "index.js",
  "files": [
    "index.js",
    ".eslintrc.js",
    "tsconfig.json",
    "readme.md"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "update": "pnpm update --latest --interactive",
    "release": "npm publish --access public",
    "flush": "rm -rf node_modules package-lock.json"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^15.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.5.5"
  }
}
