{
  "name": "@dija/eslint-config",
  "version": "3.0.0",
  "description": "Dija ESLint and Prettier Configs for Javascript and Typescript packages.",
  "keywords": [
    "javascript",
    "typescript",
    "eslint",
    "prettier"
  ],
  "author": "Dija Team <info@dija.sa>",
  "license": "MIT",
  "repository": "https://github.com/dija/eslint-config",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "index.mjs",
  "types": "index.ts",
  "type": "module",
  "files": [
    "index.mjs",
    "index.ts",
    "tsconfig.json",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write .",
    "release": "npm publish --access public",
    "flush": "rm -rf node_modules package-lock.json"
  },
  "dependencies": {
    "typescript-eslint": "^8.32.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": ">=18.0.0",
    "typescript": ">=5.0.0"
  }
}
